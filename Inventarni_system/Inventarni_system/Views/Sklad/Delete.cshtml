@model Inventarni_system.Models.Sklad

<h2>Potvrzení smazání</h2>

<p>Opravdu chcete smazat sklad <strong>@Model.Nazev</strong>?</p>

<form asp-action="Delete" method="post">
    @Html.AntiForgeryToken()
    <input type="hidden" asp-for="Id" />
    <button type="submit" class="btn btn-danger">Ano, smažte</button>
    <a asp-action="Index" class="btn btn-secondary">Zrušit</a>
</form>
